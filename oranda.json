    "social": {
      "image": "https://www.axo.dev/meta_small.jpeg",
      "image_alt": "axo",
      "twitter_account": "@axodotdev"
    },
    "analytics": {
      "plausible": {
        "domain": "opensource.axo.dev"
      }
    }
  },
  "styles": {
    "theme": "dark",
    "favicon": "https://www.axo.dev/favicon.ico"
  },
  "components": {
    "mdbook": {
      "theme": true
    },
    "changelog": true,
    "funding": {
      "yml_path": ".github/FUNDING.yml"
    },
    "artifacts": {
      "cargo_dist": true,
      "package_managers": {
        "npx": "npx @axodotdev/axolotlsay",
