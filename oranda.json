      }
    }
  },
  "build": {
    "dist_dir": "./public/",
    "static_dir": "./static/",
    "path_prefix": "axolotlsay"
  },
  "project": {
    "name": "axolotlsay",
    "repository": "https://github.com/ashleygwilliams/axolotlsay",
